  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "apollo:introspect-schema":
      "apollo-codegen introspect-schema http://localhost:8000/___graphql --output schema.json",
    "apollo:generate":
      "apollo-codegen generate src/**/*.tsx --schema schema.json --target typescript-modern --tag-name=graphql --add-typename",
    "apollo": "yarn apollo:introspect-schema && yarn apollo:generate",
    "tsc-watch": "tsc -w --noEmit"
  }
