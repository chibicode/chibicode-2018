  "scripts": {
    "new": "node scripts/newPost.js",
    "dev": "gatsby develop",
    "start": "concurrently --kill-others \"yarn dev\" \"yarn tsc-watch\"",
    "build": "gatsby build",
    "apollo:introspect-schema": "apollo-codegen introspect-schema http://localhost:8000/___graphql --output schema.json",
